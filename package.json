    "@react-three/drei": "^7.0.3",
    "@react-three/fiber": "^7.0.2",
    "@supabase/supabase-js": "^1.17.0",
    "inter-ui": "^3.19.2",
    "leva": "^0.9.13",
    "lodash-es": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serve": "^12.0.0",
    "three": "^0.130.0",
    "zustand": "^3.5.5"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/three": "^0.129.1",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@vitejs/plugin-react-refresh": "^1.3.4",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.4",
    "vite": "^2.3.8",
    "vite-react-jsx": "^1.1.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "eslint": "eslint --cache src",
    "eslint-fix": "eslint --fix --cache src",
    "prettier": "prettier --list-different '**/*.{js,jsx,md,ts,tsx}'",
    "prettier-fix": "prettier --write '**/*.{js,jsx,md,ts,tsx}'",
    "prepare": "husky install"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,md}": "prettier --write"
  }
